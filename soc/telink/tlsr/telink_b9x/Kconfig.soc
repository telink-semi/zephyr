config SOC_RISCV_TELINK_B9X
	bool
	select SOC_FAMILY_TELINK_TLSR
	help
	  Telink B9X

choice
prompt "Telink B9x SoCs implementation"

config SOC_RISCV_TELINK_B91
	bool "Telink B91 SoC implementation"
	select SOC_RISCV_TELINK_B9X
	help
	  Telink B91

config SOC_RISCV_TELINK_B92
	bool "Telink B92 SoC implementation"
	select SOC_RISCV_TELINK_B9X
	help
	  Telink B92

config SOC_RISCV_TELINK_B95
	bool "Telink B95 SoC implementation"
	select SOC_RISCV_TELINK_B9X
	help
	  Telink B95

endchoice

config SOC_SERIES
	default "telink_b9x" if SOC_RISCV_TELINK_B9X

config SOC
	default "telink_b91" if SOC_RISCV_TELINK_B91
	default "telink_b92" if SOC_RISCV_TELINK_B92
	default "telink_b95" if SOC_RISCV_TELINK_B95
